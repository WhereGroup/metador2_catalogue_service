<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <!-- SERVICE -->
        <parameter key="catalogue_service.csw.class">Plugins\WhereGroup\CatalogueServiceBundle\Component\Csw</parameter>
    </parameters>
    <services>
        <!-- SERVICE -->
        <service id="catalogue_service" class="%catalogue_service.csw.class%">
            <argument type="service" id="service_container" />
            <argument type="service" id="request_stack" />
            <argument type="service" id="metador_metadata" />
            <argument type="service" id="metador_plugin" />
            <argument type="service" id="templating" />
        </service>
    </services>
</container>
